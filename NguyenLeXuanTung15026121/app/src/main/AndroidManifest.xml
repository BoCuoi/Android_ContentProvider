<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nguyenlexuantung15026121.finalexam">

    <permission
        android:name="nguyenlexuantung15026121.finalexam.WRITE_DATABASE"
        android:label="Write Database"
        android:protectionLevel="normal" />
    <permission
        android:name="nguyenlexuantung15026121.finalexam.READ_DATABASE"
        android:label="Read Database"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ProductActivity"></activity>
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.AddActivity" />

        <provider
            android:name=".DbContentProvider"
            android:authorities="nguyenlexuantung15026121.finalexam"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>